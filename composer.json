{
    "name": "sylius/categorizer-bundle",
    "type": "symfony-bundle",
    "description": "Easy categorizing in Symfony2 applications. Categorize any model you want, multiple catalogs supported.",
    "keywords": ["category", "categories", "catalog"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "email": "pjedrzejewski@diweb.pl",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Sylius/SyliusCategorizerBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",

        "symfony/symfony":                 "2.1.*",
        "white-october/pagerfanta-bundle": "*"
    },
    "require-dev": {
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-0": { "Sylius\\Bundle\\CategorizerBundle": "" }
    },
    "target-dir": "Sylius/Bundle/CategorizerBundle"
}
