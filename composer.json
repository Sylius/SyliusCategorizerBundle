{
    "name": "sylius/catalog-bundle",
    "type": "symfony-bundle",
    "description": "Easy categorizing in Symfony2 applications. Categorize any model you want, multiple catalogs supported.",
    "keywords": ["category", "categories", "catalog"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "email": "pjedrzejewski@diweb.pl"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Sylius/SyliusCatalogBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.*"
    },
    "suggest": {
        "symfony/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Sylius\\Bundle\\CatalogBundle": "" }
    },
    "target-dir": "Sylius/Bundle/CatalogBundle"
}
